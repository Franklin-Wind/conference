<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.franklin.conference.fragment.EstablishFragment">




    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView
            android:id="@+id/establish_conference"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="会议内容" />
        <EditText
            android:id="@+id/establish_conference_write"
            android:hint="在这里输入"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

    </LinearLayout>

    <Button
        android:id="@+id/establish_btn_create"
        android:layout_width="wrap_content"
        android:layout_height="79dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="137dp"
        android:layout_marginTop="161dp"
        android:text="发布会议" />


    <Spinner
        android:id="@+id/spinner"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/establish_btn_create" />

</RelativeLayout>
